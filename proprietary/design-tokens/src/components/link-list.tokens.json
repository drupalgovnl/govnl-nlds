{
  "govnl": {
    "link-list": {
      "row-gap": {
        "$type": "spacing",
        "$value": "{govnl.gap-content.min} / 2"
      },
      "icon": {
        "inset-block-start": {
          "$type": "spacing",
          "$value": "0px"
        },
        "size": {
          "$type": "sizing",
          "$value": "1.125em"
        }
      },
      "link": {
        "column-gap": {
          "$type": "spacing",
          "$value": "{govnl.gap-content.min} / 2"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "None"
        },
        "hover": {
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "underline"
          }
        }
      },
      "font-weight": {
        "$type": "fontWeights",
        "$value": "{govnl.document.font-weight}"
      }
    }
  }
}
{
  "govnl": {
    "page-footer": {
      "background-color": {
        "$type": "color",
        "$value": "{govnl.root.background-color}"
      },
      "color": {
        "$type": "color",
        "$value": "{govnl.brand.primary.500}"
      },
      "padding-block-start": {
        "$type": "dimension",
        "$value": "{govnl.space.2xl}"
      },
      "padding-block-end": {
        "$type": "dimension",
        "$value": "{govnl.space.2xl}"
      },
      "content": {
        "column-gap": {
          "$type": "dimension",
          "$value": "calc(2 * {govnl.gap-content.min})"
        },
        "row-gap": {
          "$type": "dimension",
          "$value": "calc(1.5 * {govnl.gap-content.min})"
        }
      },
      "border-width": {
        "$type": "borderWidth",
        "$value": "{govnl.border.border-width.sm}"
      },
      "border-color": {
        "$type": "color",
        "$value": "{govnl.brand.primary.200}"
      }
    }
  }
}
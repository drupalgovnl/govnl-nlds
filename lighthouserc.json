{
  "ci": {
    "collect": {
      "staticDistDir": "./packages/storybook/dist",
      "url": [
        "http://localhost/index.html",
        "http://localhost/iframe.html?id=components-button--default",
        "http://localhost/iframe.html?id=components-heading--default",
        "http://localhost/iframe.html?id=components-link--default"
      ]
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.8 }],
        "categories:accessibility": ["error", { "minScore": 0.9 }],
        "categories:best-practices": ["warn", { "minScore": 0.8 }],
        "categories:seo": ["warn", { "minScore": 0.8 }]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}

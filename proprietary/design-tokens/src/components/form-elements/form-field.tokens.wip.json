{
  "govnl": {
    "form-field-description": {
      "font-family": {
        "$type": "fontFamilies",
        "$value": "{govnl.form-control.font-family}"
      },
      "font-size": {
        "$type": "fontSizes",
        "$value": "{govnl.typography.font-size.sm}"
      },
      "font-weight": {
        "$type": "fontWeights",
        "$value": "{govnl.form-control.font-weight}"
      },
      "color": {
        "$type": "color",
        "$value": "{govnl.document.subtle.color}"
      }
    },
    "form-field-error-message": {
      "font-family": {
        "$type": "fontFamilies",
        "$value": "{govnl.form-control.font-family}"
      },
      "font-size": {
        "$type": "fontSizes",
        "$value": "{govnl.typography.font-size.sm}"
      },
      "font-weight": {
        "$type": "fontWeights",
        "$value": "{govnl.form-control.font-weight}"
      },
      "icon": {
        "size": {
          "$type": "sizing",
          "$value": "{govnl.icon.size}"
        },
        "color": {
          "$type": "color",
          "$value": "{govnl.form-control.invalid.color}"
        }
      },
      "column-gap": {
        "$type": "dimension",
        "$value": "{govnl.space.3xs}"
      },
      "color": {
        "$type": "color",
        "$value": "{govnl.form-control.invalid.color}"
      }
    },
    "form-field-label-group": {
      "row-gap": {
        "$type": "dimension",
        "$value": "0rem"
      }
    },
    "form-field": {
      "row-gap": {
        "$type": "dimension",
        "$value": "{govnl.space.2xs}"
      },
      "error": {
        "padding-inline-start": {
          "$type": "dimension",
          "$value": "{govnl.space.sm}"
        },
        "border-inline-start-width": {
          "$type": "borderWidth",
          "$value": "{govnl.border.border-width.md}"
        },
        "border-color": {
          "$type": "color",
          "$value": "{govnl.form-control.invalid.border-color}"
        }
      }
    }
  }
}
{
  "govnl": {
    "searchbar": {
      "input": {
        "border": {
          "border-radius": {
            "$type": "borderRadius",
            "$value": "{govnl.border.border-radius.sm}"
          },
          "border-color": {
            "$type": "color",
            "$value": "{govnl.form-control.border-color}"
          },
          "border-width": {
            "$type": "borderWidth",
            "$value": "{govnl.border.border-width.sm}"
          }
        },
        "padding-block-start": {
          "$type": "dimension",
          "$value": "{govnl.space.xs}"
        },
        "padding-block-end": {
          "$type": "dimension",
          "$value": "{govnl.space.xs}"
        },
        "padding-inline-start": {
          "$type": "dimension",
          "$value": "{govnl.space.sm}"
        },
        "padding-inline-end": {
          "$type": "dimension",
          "$value": "{govnl.space.sm}"
        },
        "placeholder": {
          "color": {
            "$type": "color",
            "$value": "{govnl.form-control.placeholder.color}"
          }
        }
      },
      "gap": {
        "$type": "dimension",
        "$value": "{govnl.dimension.size.50}"
      }
    },
    "input": {
      "placeholder": {
        "font-size": {
          "$type": "fontSizes",
          "$value": "{govnl.document.font-size}"
        }
      }
    }
  },
  "searchbar": {
    "input": {
      "placeholder": {
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{govnl.document.font-family}"
        }
      }
    }
  }
}
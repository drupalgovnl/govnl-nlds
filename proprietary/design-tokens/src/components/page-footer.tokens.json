{
  "govnl": {
    "page-footer": {
      "background-color": {
        "$type": "color",
        "$value": "{govnl.root.background-color}"
      },
      "color": {
        "$type": "color",
        "$value": "{govnl.brand.primary.500}"
      },
      "padding-block-start": {
        "$type": "dimension",
        "$value": "{govnl.space.2xl}"
      },
      "padding-block-end": {
        "$type": "dimension",
        "$value": "{govnl.space.2xl}"
      },
      "container": {
        "max-inline-width": {
          "$type": "dimension",
          "$value": "{govnl.container.max-inline-size}"
        },
        "padding-inline": {
          "$type": "dimension",
          "$value": "{govnl.container.padding-inline}"
        }
      },
      "content": {
        "column-gap": {
          "$type": "dimension",
          "$value": "{govnl.gap-content.min} * 2"
        },
        "row-gap": {
          "$type": "dimension",
          "$value": "{govnl.gap-content.min} * 1.5"
        }
      },
      "border-width": {
        "$type": "borderWidth",
        "$value": "{govnl.border.border-width.sm}"
      },
      "border-color": {
        "$type": "color",
        "$value": "{govnl.brand.primary.200}"
      }
    }
  }
}

{
  "govnl": {
    "nav-bar": {
      "border-block-end-width": {
        "$type": "borderWidth",
        "$value": "{govnl.border.border-width.sm}"
      },
      "max-inline-size": {
        "$type": "sizing",
        "$value": "1280px"
      },
      "padding-inline": {
        "$type": "spacing",
        "$value": "{govnl.space.sm}"
      },
      "container": {
        "inline-size": {
          "$type": "sizing",
          "$value": "100%"
        }
      },
      "content": {
        "column-gap": {
          "$type": "spacing",
          "$value": "{govnl.gap-content.min}"
        }
      },
      "icon": {
        "color": {
          "$type": "color",
          "$value": "{govnl.brand.primary.500}"
        },
        "active": {
          "color": {
            "$type": "color",
            "$value": "{govnl.brand.primary.500}"
          }
        },
        "size": {
          "$type": "sizing",
          "$value": "1.5em"
        }
      },
      "link": {
        "active": {
          "background-color": {
            "$type": "color",
            "$value": "{govnl.brand.primary.500}"
          },
          "color": {
            "$type": "color",
            "$value": "{govnl.document.inverse.color}"
          }
        },
        "background-color": {
          "$type": "color",
          "$value": "{govnl.document.background-color}"
        },
        "color": {
          "$type": "color",
          "$value": "{govnl.brand.primary.500}"
        },
        "focus": {
          "background-color": {
            "$type": "color",
            "$value": "{govnl.brand.primary.50}"
          },
          "color": {
            "$type": "color",
            "$value": "{govnl.brand.primary.500}"
          }
        },
        "hover": {
          "background-color": {
            "$type": "color",
            "$value": "{govnl.brand.primary.50}"
          },
          "color": {
            "$type": "color",
            "$value": "{govnl.brand.primary.500}"
          }
        },
        "padding-block": {
          "$type": "spacing",
          "$value": "{govnl.space.sm}"
        },
        "padding-inline": {
          "$type": "spacing",
          "$value": "{govnl.space.xs}"
        },
        "column-gap": {
          "$type": "spacing",
          "$value": "{govnl.gap-content.min} / 2"
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "none"
        }
      },
      "background-color": {
        "$type": "color",
        "$value": "{govnl.document.background-color}"
      },
      "color": {
        "$type": "color",
        "$value": "{govnl.brand.primary.500}"
      },
      "border-color": {
        "$type": "color",
        "$value": "{govnl.brand.primary.400}"
      },
      "search": {
        "padding-block": {
          "$type": "spacing",
          "$value": "{govnl.space.xs}"
        },
        "padding-inline": {
          "$type": "spacing",
          "$value": "{govnl.space.xs}"
        }
      },
      "menu-label": {
        "gap": {
          "$type": "spacing",
          "$value": "{govnl.gap-content.min} / 2"
        },
        "padding-inline": {
          "$type": "spacing",
          "$value": "{govnl.space.sm}"
        },
        "padding-block": {
          "$type": "spacing",
          "$value": "{govnl.space.2xs}"
        },
        "color": {
          "$type": "color",
          "$value": "{govnl.brand.primary.500}"
        },
        "background-color": {
          "$type": "color",
          "$value": "none"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{govnl.document.font-size}"
        }
      },
      "mobile": {
        "max-inline-size": {
          "$type": "sizing",
          "$value": "100%"
        }
      },
      "font-size": {
        "$type": "fontSizes",
        "$value": "{govnl.document.font-size}"
      }
    }
  }
}

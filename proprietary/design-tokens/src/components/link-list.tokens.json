{
  "components/link-list": {
    "utrecht": {
      "link-list": {
        "row-gap": {
          "$type": "dimension",
          "$value": "{govnl.space.row.200}"
        },
        "icon": {
          "inset-block-start": {
            "$type": "dimension",
            "$value": "0px"
          },
          "size": {
            "$type": "dimension",
            "$value": "{govnl.icon.functional.size}"
          }
        },
        "link": {
          "column-gap": {
            "$type": "dimension",
            "$value": "{govnl.space.block.200}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "None"
          }
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{utrecht.document.font-weight}"
        }
      }
    },
    "todo": {
      "link-list": {
        "link": {
          "hover": {
            "text-decoration": {
              "$type": "textDecoration",
              "$value": "underline"
            }
          }
        }
      }
    }
  }
}
{
  "name": "@dictu/storybook",
  "type": "module",
  "author": "Dienst ICT Uitvoering",
  "description": "Storybook based on the NL Design System architecture",
  "license": "EUPL-1.2",
  "keywords": [
    "nl-design-system"
  ],
  "private": true,
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:drupalgovnl/govnl-nlds.git",
    "directory": "packages/storybook"
  },
  "scripts": {
    "build": "storybook build --output-dir dist/ --config-dir config/ --quiet",
    "clean": "rimraf dist/",
    "storybook": "storybook dev --config-dir config/ --port 6006"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@storybook/addon-a11y": "10.1.4",
    "@storybook/addon-docs": "10.1.4",
    "@storybook/addon-links": "10.1.4",
    "@storybook/builder-vite": "10.1.4",
    "@storybook/html-vite": "10.1.4",
    "axe-core": "^4.10.3",
    "rimraf": "6.0.1",
    "rollup": "^4.44.0",
    "storybook": "10.1.4",
    "storybook-addon-tag-badges": "^3.0.2",
    "ts-dedent": "^2.2.0",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@dictu/design-tokens": "*"
  },
  "overrides": {
    "storybook": "$storybook"
  }
}

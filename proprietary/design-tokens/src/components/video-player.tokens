{
  "govnl": {
    "video-player": {
      "row-gap": {
        "$type": "dimension",
        "$value": "{govnl.space.3xs}"
      },
      "player-controls": {
        "color": {
          "$type": "color",
          "$value": "{govnl.interaction.color}"
        },
        "background-color": {
          "$type": "color",
          "$value": "{govnl.root.background-color}"
        },
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{govnl.document.font-family}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{govnl.document.font-weight}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{govnl.document.font-size}"
        },
        "line-height": {
          "$type": "fontHeights",
          "$value": "{govnl.document.line-height}"
        },
        "padding-block-start": {
          "$type": "dimension",
          "$value": "{govnl.space.2xs}"
        },
        "padding-block-end": {
          "$type": "dimension",
          "$value": "{govnl.space.2xs}"
        },
        "padding-inline-start": {
          "$type": "dimension",
          "$value": "{govnl.space.2xs}"
        },
        "padding-inline-end": {
          "$type": "dimension",
          "$value": "{govnl.space.2xs}"
        },
        "column-gap": {
          "$type": "dimension",
          "$value": "{govnl.space.2xs}"
        },
        "progress-indicator": {
          "color": {
            "$type": "color",
            "$value": "{govnl.brand.primary-1.500}"
          }
        },
        "progress-bar": {
          "color": {
            "$type": "color",
            "$value": "{govnl.brand.primary-1.200}"
          }
        }
      }
    }
  }
}

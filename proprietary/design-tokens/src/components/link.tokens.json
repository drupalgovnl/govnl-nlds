{
  "nl": {
    "link": {
      "color": {
        "value": "{govnl.interaction.color}",
        "type": "color"
      },
      "text-decoration-color": {
        "value": "{govnl.interaction.color}",
        "type": "color"
      },
      "text-decoration-line": {
        "value": "underline",
        "type": "textDecoration"
      },
      "text-decoration-thickness": {
        "value": "auto",
        "type": "other"
      },
      "text-underline-offset": {
        "value": "auto",
        "type": "other"
      },
      "active": {
        "color": {
          "value": "{govnl.interaction.color}",
          "type": "color",
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "darken",
                "value": "0.4",
                "space": "srgb"
              }
            }
          }
        },
        "text-decoration-line": {
          "value": "None",
          "type": "textDecoration"
        },
        "text-decoration-thickness": {
          "value": "auto",
          "type": "other"
        }
      },
      "current": {
        "cursor": {
          "value": "default",
          "type": "other"
        }
      },
      "disabled": {
        "color": {
          "value": "{govnl.color.grijs.500}",
          "type": "color"
        },
        "cursor": {
          "value": "disabled",
          "type": "other"
        }
      },
      "focus-visible": {
        "background-color": {
          "value": "{govnl.focus.background-color}",
          "type": "color"
        },
        "color": {
          "value": "{govnl.focus.color}",
          "type": "color"
        },
        "text-decoration-line": {
          "value": "None",
          "type": "textDecoration"
        },
        "text-decoration-thickness": {
          "value": "auto",
          "type": "other"
        }
      },
      "hover": {
        "color": {
          "value": "{govnl.interaction.color}",
          "type": "color",
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "darken",
                "value": "0.2",
                "space": "srgb"
              }
            }
          }
        },
        "text-decoration-line": {
          "value": "None",
          "type": "textDecoration"
        },
        "text-decoration-thickness": {
          "value": "auto",
          "type": "other"
        }
      }
    }
  }
}
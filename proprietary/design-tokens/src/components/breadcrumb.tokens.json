{
  "govnl": {
    "breadcrumb-nav": {
      "font-family": {
        "$type": "fontFamilies",
        "$value": "{govnl.document.font-family}"
      },
      "font-size": {
        "$type": "fontSizes",
        "$value": "{govnl.document.font-size}"
      },
      "font-weight": {
        "$type": "fontWeights",
        "$value": "{govnl.document.font-weight}"
      },
      "line-height": {
        "$type": "lineHeights",
        "$value": "{govnl.document.line-height}"
      },
      "link": {
        "padding-block-end": {
          "$type": "dimension",
          "$value": "{govnl.space.xs}"
        },
        "padding-block-start": {
          "$type": "dimension",
          "$value": "{govnl.space.xs}"
        },
        "padding-inline-end": {
          "$type": "dimension",
          "$value": "{govnl.space.xs}"
        },
        "padding-inline-start": {
          "$type": "dimension",
          "$value": "0px {govnl.space.xs}"
        },
        "color": {
          "$type": "color",
          "$value": "{govnl.interaction.color}"
        },
        "active": {
          "color": {
            "$type": "color",
            "$value": "{govnl.interaction.active.color}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "None"
          }
        },
        "focus-visible": {
          "background-color": {
            "$type": "color",
            "$value": "{govnl.focus.background-color}"
          },
          "color": {
            "$type": "color",
            "$value": "{govnl.focus.color}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "None"
          }
        },
        "hover": {
          "color": {
            "$type": "color",
            "$value": "{govnl.interaction.hover.color}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "None"
          }
        },
        "current": {
          "color": {
            "$type": "color",
            "$value": "{govnl.document.subtle.color}"
          }
        },
        "text-decoration": {
          "$type": "textDecoration",
          "$value": "underline"
        }
      },
      "separator": {
        "color": {
          "$type": "color",
          "$value": "{govnl.document.subtle.color}"
        },
        "size": {
          "$type": "dimension",
          "$value": "{govnl.dimension.size.300}"
        }
      }
    }
  }
}

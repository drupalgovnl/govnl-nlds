        "active": {
          "background-color": {
            "$type": "color",
            "$value": "{govnl.interaction.active.color}"
          },
          "border-color": {
            "$type": "color",
            "$value": "transparant"
          },
          "color": {
            "$type": "color",
            "$value": "{govnl.document.inverse.color}"
          },
          "border-width": {
            "$type": "borderWidth",
